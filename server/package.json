{
  "name": "maa-json-viewer-server",
  "scripts": {
    "build": "esbuild --bundle src/index.ts --outfile=dist/index.js --sourcemap --platform=node",
    "run": "npm run build && node dist/index.js"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.17.2",
    "esbuild": "^0.18.17",
    "express": "^4.18.2",
    "pkg": "^5.8.1",
    "typescript": "^5.1.6"
  },
  "bin": "dist/index.js",
  "pkg": {
    "targets": [
      "node18-win-x64",
      "node18-linux-x64",
      "node18-macos-x64"
    ],
    "outputPath": "out"
  }
}
